<?xml version="1.0" encoding="UTF-8"?>
<vehicles>

	<vehicle>
		<vehicleType>Big Wheel</vehicleType>
		<frame>
			<material>plastic</material>
		</frame>
		<wheels>
			<wheel>
				<position>left rear</position>
				<material>plastic</material>
			</wheel>
			<wheel>
				<position>right rear</position>

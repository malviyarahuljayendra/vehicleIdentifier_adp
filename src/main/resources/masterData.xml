			</wheel>
			<wheel>
				<position>front</position>
				<material>metal</material>
			</wheel>
		</wheels>
		<powertrain>
			<human />
		</powertrain>
	</vehicle>
	
	<vehicle>
		<vehicleType>Motorcycle</vehicleType>
		<frame>
			<material>metal</material>
		</frame>
		<wheels>
			<wheel>
				<position>rear</position>
				<material>metal</material>
			</wheel>
			<wheel>
				<position>front</position>
				<material>metal</material>
			</wheel>
		</wheels>
		<powertrain>
			<internalCombustion />
		</powertrain>
	</vehicle>
	
	<vehicle>
		<vehicleType>Hang Glider</vehicleType>

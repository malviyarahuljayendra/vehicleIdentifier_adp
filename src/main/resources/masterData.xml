			<material>plastic</material>
		</frame>
		<wheels>
			
		</wheels>
		<powertrain>
			<bernoulli />
		</powertrain>
	</vehicle>
	
	<vehicle>
		<vehicleType>Car</vehicleType>
		<frame>
			<material>metal</material>
		</frame>
		<wheels>
			<wheel>
				<position>left rear</position>
				<material>metal</material>
			</wheel>
			<wheel>
				<position>left front</position>
				<material>metal</material>
			</wheel>
			
			<wheel>
				<position>right rear</position>
				<material>metal</material>
			</wheel>
			<wheel>
